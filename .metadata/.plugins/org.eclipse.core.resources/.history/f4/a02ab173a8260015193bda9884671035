
public class Student {
	private static int id = 130000;
	private String name;
	private String classNum;
	private String address;
	
	public Student(){
		name = "이름 없음";
		classNum = "학과 없음";
		address = "주소 없음";
	}
	
	public Student(String name, String classNum, String address){
		this.id = id++;
		this.name = name;
		this.classNum = classNum;
		this.address = address;
	}
}
