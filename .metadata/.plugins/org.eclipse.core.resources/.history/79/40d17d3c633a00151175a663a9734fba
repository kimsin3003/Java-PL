
public class Student {
	String name;
	String id;
	String major;
	int mathScore;
	int englishScore;
	
	Student(String name, String id, String major){
		this.name = name;
		this.id = id;
		this.major =  major;
	}
	
	char getMathGrade(){
		if(major == "math")
			return Major.getGrade(mathScore);
		
		else
			return NotMajor.getGrade(mathScore);
	}
	
	

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getMajor() {
		return major;
	}

	public void setMajor(String major) {
		this.major = major;
	}

	public int getMathScore() {
		return mathScore;
	}

	public void setMathScore(int math) {
		this.mathScore = math;
	}

	public int getEnglish() {
		return english;
	}

	public void setEnglish(int english) {
		this.english = english;
	}

}
