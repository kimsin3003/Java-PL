
public class Major implements Strategy {

	@Override
	public static char computeGrade(int score) {
		
		if(score < 60)
			return 'F';
		
		else if(score < 70)
			return 'D';
		
		else if(score < 80)
			return 'C';
		
		else if(score < 90)
			return 'D';
		
		else if(score <= 100)
			return 'A';
		
		return 'X';
	}

}
