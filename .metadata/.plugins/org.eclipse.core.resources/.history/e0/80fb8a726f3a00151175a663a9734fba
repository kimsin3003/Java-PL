import java.util.ArrayList;

public abstract class Major implements Strategy{
	
	ArrayList<Student> students = new ArrayList<Student>();

	
	void enroll(Student newStu){
		students.add(newStu);
	}
	
	void drop(Student deadStu){
		students.remove(deadStu);
	}
	
	abstract void setScore(Student st, int score);
	
	void setGrade(){
		for(int i = 0; i < students.size(); i++)
			students.get(i)
	}
	
	abstract void computeGrade(Student stu);

	@Override
	public char getMajorGrade(Student student) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public char getNonMajorGrade(Student student) {
		// TODO Auto-generated method stub
		return 0;
	}
