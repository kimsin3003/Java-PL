public class English extends Class{
	
	@Override
	public void setScore(Student st, int score) {
		st.setEngScore(score);
	}

	@Override
	public void computeGrade(Student student) {
		if(student.getMajor() == "English")
			getMajorGrade(student.getEngScore());
		
		else
			getNonMajorGrade(student.getEngScore());
		
	}

}
