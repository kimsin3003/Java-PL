
public class SutdaCard {
	int num;
	boolean isKwang;
	
	public SutdaCard(){
		num = -1;
		isKwang = true;
	}
	
	public SutdaCard(int num, boolean isKwang){
		this.num = num;
		this.isKwang = isKwang;
	}
	
	public void info(){
		String result = "";
		if(num != -1)
			result += num;
		if(isKwang)
			result += "IK");
	}
	
}
