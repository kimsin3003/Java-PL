public class Math extends Class{
	
	@Override
	void setScore(Student st, int score) {
		st.setMathScore(score);
	}

	@Override
	public void computeGrade(Student student) {
		if(student.getMajor() == "Math")
			getMajorGrade(student.getMathScore());
		
		else
			getNonMajorGrade(student.getMathScore());
		
	}

	@Override
	void setGrade() {
		for(int i = 0; i < students.size(); i++)
			computeGrade(students.get(i));
	}

}
