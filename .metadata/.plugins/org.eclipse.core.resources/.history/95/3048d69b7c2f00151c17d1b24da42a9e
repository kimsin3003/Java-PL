
public abstract class GameLevel {
	
	int level;
	public GameLevel(){}
	
	public abstract void simpleAttack();
	public abstract void turnAttack();
	public abstract void flyingAttack();
	
	final void play(){
		System.out.println("level" + level);
		simpleAttack();
		System.out.println("level" + level);
		turnAttack();
		flyingAttack();
	}
}
