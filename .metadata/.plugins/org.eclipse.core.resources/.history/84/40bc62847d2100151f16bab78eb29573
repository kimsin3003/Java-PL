
public class Birthday {
	private int year;
	private int month;
	private int day;
	
	public Birthday(int year, int month, int day){
		this.year = year;
		this.month = month;
		this.day = day;
	}
	
	public boolean validity(){
		
		
		
	}
	
	private boolean isLeapYear(){
		if(year % 400 == 0) return true;
		else if(year % 100 == 0) return false;
		else if(year % 400 == 0) return true;
		else return false;
	}
}
