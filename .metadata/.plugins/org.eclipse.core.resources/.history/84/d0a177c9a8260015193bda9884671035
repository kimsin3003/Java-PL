
public class Student {
	private int id = 130000;
	private String name;
	private String major;
	private String address;
	
	public Student(){
		id++;
		name = "이름 없음";
		major = "학과 없음";
		address = "주소 없음";
	}
	
	public Student(String name, String major, String address){
		this.id = id++;
		this.name = name;
		this.major = major;
		this.address = address;
	}
}
