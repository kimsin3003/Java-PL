
public class SutdaCard {
	int num;
	boolean isKwang;
	
	public SutdaCard(){
		num = -1;
		isKwang = true;
	}
	
	public SutdaCard(int num, boolean isKwang){
		this.num = num;
		this.isKwang = isKwang;
	}
	
	public void info(){
		if(num != -1)
			System.out.println(num);
		if(isKwang)
			System.out.println("IK");
	}
	
}
