import java.util.ArrayList;

public class Math extends Class{
	
	@Override
	void setScore(Student st, int score) {
		st.mathScore = score;
	}

	@Override
	public void computeGrade(Student stu) {
		
		
	}

	@Override
	public char getMajorGrade(int score) {
		if(score < 60)
			return 'F';
		
		else if(score < 70)
			return 'D';
		
		else if(score < 80)
			return 'C';
		
		else if(score < 90)
			return 'B';
		
		else if(score <= 100)
			return 'A';
		
		else{
			System.out.println("Too high as a score");
		}
	}

	@Override
	public char getNonMajorGrade(int score) {
		// TODO Auto-generated method stub
		return 0;
	}

}
