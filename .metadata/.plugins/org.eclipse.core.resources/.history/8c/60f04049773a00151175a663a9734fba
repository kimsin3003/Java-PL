
public class School {

	public static void main(String[] args) {
		Math mathClass = new Math();
		English engClass = new English();
		
		Student s1 =  new Student("이승기", "141213", "수학");
		Student s2 =  new Student("김수현", "141518", "영어");
		Student s3 =  new Student("주원", "141230", "수학");
		Student s4 =  new Student("김우빈", "141255", "수학");
		Student s5 =  new Student("이민호", "141590", "영어");
		
		mathClass.enroll(s1);
		mathClass.enroll(s2);
		mathClass.enroll(s3);
		mathClass.enroll(s4);
		mathClass.enroll(s5);
		
		mathClass.setScore(s1, 95);
		mathClass.setScore(s2, 90);
		mathClass.setScore(s3, 76);
		mathClass.setScore(s4, 85);
		mathClass.setScore(s5, 90);		
		
		engClass.enroll(s1);
		engClass.enroll(s2);
		engClass.enroll(s3);
		engClass.enroll(s4);
		engClass.enroll(s5);
		
		engClass.setScore(s1, 20);
		engClass.setScore(s2, 100);
		engClass.setScore(s3, 60);
		engClass.setScore(s4, 89);
		engClass.setScore(s5, 95);
		
		
		System.out.println("이름: "+ s1 + " 학번: " + );

			

	}

}
