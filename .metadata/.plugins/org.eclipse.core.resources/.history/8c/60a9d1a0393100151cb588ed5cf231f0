class Exercise6_22 {
	
	static boolean isNumber(String str){
		if(str == null || str == "")
			return false;
		
		for(int i = 0; i < str.length(); i++){
			if(str.charAt(i) >= '0' && str.charAt(i) <= '9')
				return true;
		}
		
		return false;
	}

	public static void main(String[] args) {
		

	}
}


