import java.util.ArrayList;

public class Major implements Strategy{
	
	ArrayList<Student> students = new ArrayList<Student>();

	
	void enroll(Student newStu){
		students.add(newStu);
	}
	
	void drop(Student deadStu){
		students.remove(deadStu);
	}
	
	void setScore(Student st, int score){
		
		students.get(students.indexOf(st)).mathScore = score;
	}
	
	void setGrade(){
		
	}

	@Override
	public char getMajorGrade(Student student) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public char getNonMajorGrade(Student student) {
		// TODO Auto-generated method stub
		return 0;
	}
